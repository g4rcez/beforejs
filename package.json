  },
  "dependencies": {
    "@types/express": "4.17.13",
    "@types/node": "^16.3.1",
    "axios": "0.21.1",
    "express": "^4.17.1",
    "marked": "2.1.3",
    "node-html-parser": "4.1.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@types/glob": "7.1.4",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "autoprefixer": "10.3.1",
    "chokidar": "3.5.2",
    "glob": "7.1.7",
    "postcss": "8.3.6",
    "tailwindcss": "2.2.7",
    "ts-node-dev": "1.1.8",
    "tslib": "2.3.0",
    "typescript": "4.3.5",
    "vite": "^2.4.0"
